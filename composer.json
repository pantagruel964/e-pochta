{
    "name": "pantagruel964/e-pochta",
    "description": "A Laravel 5 package for epochta.ru API",
    "keywords": [
        "laravel",
        "epochta",
        "sms",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "pantagruel964",
            "email": "pantagruel964@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pantagruel964\\Epochta\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Pantagruel964\\Epochta\\EpochtaServiceProvider"
            ],
            "aliases": {
                "Epochta": "Pantagruel964\\Epochta\\EpochtaFacade"
            }
        }
    }
}
